{
  "name": "@celora/web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run build:sw && next build",
    "build:sw": "workbox injectManifest workbox-config.js",
    "start": "next start",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx . --fix",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "check": "npm run lint && npm run typecheck",
    "build:extension": "node scripts/build-extension.mjs",
    "generate:openapi": "tsx scripts/generate-openapi.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "mobile:install": "cd mobile && npm install",
    "mobile:ios": "cd mobile && npm run ios",
    "mobile:android": "cd mobile && npm run android",
    "mobile:start": "cd mobile && npm start"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@ledgerhq/hw-app-eth": "^6.38.2",
    "@ledgerhq/hw-app-solana": "^7.2.1",
    "@ledgerhq/hw-transport-webusb": "^6.29.0",
    "@noble/hashes": "^2.0.1",
    "@prisma/client": "^5.22.0",
    "@scure/bip32": "^2.0.1",
    "@scure/bip39": "^2.0.1",
    "@solana/spl-token": "^0.1.8",
    "@solana/wallet-adapter-base": "^0.9.27",
    "@solana/wallet-adapter-phantom": "^0.9.28",
    "@solana/wallet-adapter-react": "^0.15.39",
    "@solana/wallet-adapter-react-ui": "^0.9.39",
    "@solana/wallet-adapter-wallets": "^0.19.37",
    "@solana/web3.js": "^1.87.6",
    "@swc/helpers": "^0.5.17",
    "@types/bcryptjs": "^2.4.6",
    "@types/bitcoinjs-lib": "^5.0.0",
    "@types/dompurify": "^3.0.5",
    "@types/qrcode": "^1.5.5",
    "@types/speakeasy": "^2.0.10",
    "@types/tiny-secp256k1": "^2.0.0",
    "@vercel/analytics": "^1.5.0",
    "@vercel/blob": "^2.0.0",
    "@vercel/speed-insights": "^1.2.0",
    "@walletconnect/core": "^2.16.0",
    "@walletconnect/web3wallet": "^1.16.0",
    "axios": "^1.7.9",
    "bcryptjs": "^3.0.2",
    "bip39": "^3.1.0",
    "bitcoinjs-lib": "^6.1.5",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dompurify": "^3.2.7",
    "dotenv": "^17.2.2",
    "driver.js": "^1.3.1",
    "ecpair": "^3.0.0",
    "ethers": "^6.8.0",
    "firebase": "^12.6.0",
    "firebase-admin": "^12.7.0",
    "framer-motion": "^12.23.18",
    "graphql": "^16.12.0",
    "graphql-request": "^6.1.0",
    "jose": "^6.1.2",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.544.0",
    "next": "^15.5.4",
    "node-cron": "^3.0.3",
    "qrcode": "^1.5.4",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "recharts": "^2.15.4",
    "server-only": "^0.0.1",
    "speakeasy": "^2.0.0",
    "tailwind-merge": "^3.3.1",
    "tiny-secp256k1": "^2.2.4",
    "trezor-connect": "^8.2.12",
    "uuid": "^11.0.3",
    "workbox-window": "^7.3.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.48.0",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.19.0",
    "@types/node-cron": "^3.0.11",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "autoprefixer": "^10.4.19",
    "esbuild": "^0.27.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "postcss": "^8.4.38",
    "prettier": "^3.6.2",
    "prisma": "^5.22.0",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.4.4",
    "tsx": "^4.19.2",
    "typescript": "^5.4.0",
    "workbox-cli": "^2.1.3",
    "workbox-webpack-plugin": "^7.3.0",
    "zod-to-openapi": "^0.2.1"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=8.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.test.ts",
      "**/__tests__/**/*.test.tsx"
    ],
    "moduleNameMapping": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/__tests__/**",
      "!src/**/*.test.{ts,tsx}",
      "!src/types/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    },
    "coverageReporters": [
      "text",
      "text-summary",
      "lcov",
      "cobertura",
      "html",
      "json-summary"
    ],
    "testTimeout": 10000
  }
}
